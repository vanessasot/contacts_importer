<h1>Import Files</h1>

<table class="table">
  <thead>
    <tr>
      <th>File</th>
      <th>State</th>
    </tr>
  </thead>

  <tbody>
    <% @import_files.each do |import_file| %>
      <tr>
        <td><%= import_file.filename %></td>
        <td><%= import_file.state %></td>
      </tr>
    <% end %>
  </tbody>
</table>